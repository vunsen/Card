            android:label="@string/title_activity_car_case" >
        </activity>
        <activity
            android:name=".WebVisit"
            android:label="@string/title_activity_web_visit" >
        </activity>
        <activity
            android:name=".StartAcitivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CsActivity"
            android:label="@string/title_activity_cs" >
        </activity>
        <activity
            android:name=".Canvas"
            android:label="@string/title_activity_canvas" >
        </activity>
